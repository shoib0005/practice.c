// 61. search for an element in an array using linear search.
#include <stdio.h>

int main() {
    int n, i, key, a[100];
    printf("Enter size: ");
    scanf("%d", &n);

    printf("Enter elements: ");
    for(i=0; i<n; i++) scanf("%d", &a[i]);

    printf("Enter element to search: ");
    scanf("%d", &key);

    for(i=0; i<n; i++) {
        if(a[i] == key) {
            printf("Found at index %d\n", i);
            return 0;
        }
    }
    printf("Not found\n");
}

// 62. reverse an array without taking extra space.
#include <stdio.h>

int main() {
    int n, a[100], i, temp;
    scanf("%d", &n);
    for(i=0; i<n; i++) scanf("%d", &a[i]);

    for(i=0; i<n/2; i++) {
        temp = a[i];
        a[i] = a[n-i-1];
        a[n-i-1] = temp;
    }

    for(i=0; i<n; i++) printf("%d ", a[i]);
}

// 63. merge two arrays.
#include <stdio.h>

int main() {
    int n1, n2;
    scanf("%d", &n1);
    int a[n1];
    for(int i=0; i<n1; i++) scanf("%d", &a[i]);

    scanf("%d", &n2);
    int b[n2];
    for(int i=0; i<n2; i++) scanf("%d", &b[i]);

    int c[n1+n2], k = 0;

    for(int i=0; i<n1; i++) c[k++] = a[i];
    for(int i=0; i<n2; i++) c[k++] = b[i];

    for(int i=0; i<k; i++) printf("%d ", c[i]);
    return 0;
}

// 64. find digits that occurs the most times in an integer number.
#include <stdio.h>

int main() {
    int n, freq[10] = {0};
    scanf("%d", &n);

    if (n < 0) n = -n;

    while(n > 0) {
        freq[n % 10]++;
        n /= 10;
    }

    int maxDigit = 0;
    for(int i=1; i<10; i++)
        if(freq[i] > freq[maxDigit])
            maxDigit = i;

    printf("Most frequent digit = %d", maxDigit);
    return 0;
}

// 65. search in a sorted array using binary search.
#include <stdio.h>

int main() {
    int n, key;
    scanf("%d", &n);

    int a[n];
    for(int i=0; i<n; i++) scanf("%d", &a[i]);

    scanf("%d", &key);

    int low=0, high=n-1, mid, found=0;

    while(low <= high) {
        mid = (low+high)/2;

        if(a[mid] == key) { found=1; break; }
        else if(a[mid] < key) low = mid + 1;
        else high = mid - 1;
    }

    if(found) printf("Found at index %d", mid);
    else printf("Not found");

    return 0;
}

// 66. insert an element in a sorted array at the apprepriate position.
#include <stdio.h>

int main() {
    int n, x;
    scanf("%d", &n);

    int a[n+1];
    for(int i=0; i<n; i++) scanf("%d", &a[i]);

    scanf("%d", &x);

    int i;
    for(i=n-1; i>=0 && a[i] > x; i--)
        a[i+1] = a[i];

    a[i+1] = x;

    for(i=0; i<=n; i++)
        printf("%d ", a[i]);

    return 0;
}

// 67. insert an element in an array at a given position.
#include <stdio.h>

int main() {
    int n, pos, x;
    scanf("%d", &n);
    int a[n+1];

    for(int i=0; i<n; i++) scanf("%d", &a[i]);

    scanf("%d %d", &x, &pos); // element, position

    for(int i=n; i>=pos; i--)
        a[i] = a[i-1];

    a[pos-1] = x;

    for(int i=0; i<=n; i++)
        printf("%d ", a[i]);
    return 0;
}

// 68. delete an element from an array.
#include <stdio.h>

int main() {
    int n, pos;
    scanf("%d", &n);

    int a[n];
    for(int i=0; i<n; i++) scanf("%d", &a[i]);

    scanf("%d", &pos);

    for(int i=pos-1; i<n-1; i++)
        a[i] = a[i+1];

    for(int i=0; i<n-1; i++)
        printf("%d ", a[i]);

    return 0;
}

// 69. second largest element in an array.
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int a[n];
    for(int i=0; i<n; i++) scanf("%d", &a[i]);

    int max1 = a[0], max2 = -99999;

    for(int i=1; i<n; i++) {
        if(a[i] > max1) {
            max2 = max1;
            max1 = a[i];
        }
        else if(a[i] > max2 && a[i] != max1)
            max2 = a[i];
    }

    printf("Second largest = %d", max2);
    return 0;
}

// 70. rotate sn array to the right by k positions.
#include <stdio.h>

int main() {
    int n, k;
    scanf("%d", &n);
    int a[n];

    for(int i=0; i<n; i++) scanf("%d", &a[i]);
    scanf("%d", &k);

    k = k % n;

    int b[n];
    for(int i=0; i<n; i++)
        b[(i+k) % n] = a[i];

    for(int i=0; i<n; i++)
        printf("%d ", b[i]);

    return 0;
}

// 71. print matrix.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r; i++) {
        for(int j=0; j<c; j++)
            printf("%d ", a[i][j]);
        printf("\n");
    }
    return 0;
}

// 72. sum of all elements in a matrix.
#include <stdio.h>

int main() {
    int r, c, sum = 0;
    scanf("%d %d", &r, &c);

    int a[r][c];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++) {
            scanf("%d", &a[i][j]);
            sum += a[i][j];
        }

    printf("Sum = %d", sum);
    return 0;
}

// 73. sum of each row of a matrix and store it in an array.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c], rowSum[r];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r; i++) {
        rowSum[i] = 0;
        for(int j=0; j<c; j++)
            rowSum[i] += a[i][j];
    }

    for(int i=0; i<r; i++)
        printf("%d ", rowSum[i]);

    return 0;
}

// 74. transposa of a matrix.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c], t[c][r];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            t[j][i] = a[i][j];

    for(int i=0; i<c; i++) {
        for(int j=0; j<r; j++)
            printf("%d ", t[i][j]);
        printf("\n");
    }
    return 0;
}

// 75. add two matriz.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c], b[r][c], s[r][c];

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &b[i][j]);

    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            s[i][j] = a[i][j] + b[i][j];

    for(int i=0; i<r; i++) {
        for(int j=0; j<c; j++)
            printf("%d ", s[i][j]);
        printf("\n");
    }
    return 0;
}

// 76. check symmetric matrix.
#include <stdio.h>

int main() {
    int n, flag=1;
    scanf("%d", &n);

    int a[n][n];
    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            if(a[i][j] != a[j][i]) flag = 0;

    if(flag) printf("Symmetric");
    else printf("Not symmetric");

    return 0;
}

// 77. check distinct diagonal element.
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int a[n][n], used[n], flag=1;

    for(int i=0; i<n; i++) used[i] = 0;

    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<n; i++) {
        for(int j=i+1; j<n; j++)
            if(a[i][i] == a[j][j])
                flag = 0;
    }

    if(flag) printf("Distinct");
    else printf("Not distinct");

    return 0;
}

// 78. sum of main diagonal elements for a square matrix.
#include <stdio.h>

int main() {
    int n, sum = 0;
    scanf("%d", &n);

    int a[n][n];
    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<n; i++)
        sum += a[i][i];

    printf("Sum = %d", sum);
    return 0;
}

// 79. perform diagonal traversal of a matrix.
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c];
    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++)
            scanf("%d", &a[i][j]);

    // Upper diagonals including main
    for(int start=0; start<c; start++) {
        int i=0, j=start;
        while(i<r && j>=0) {
            printf("%d ", a[i][j]);
            i++; j--;
        }
        printf("\n");
    }

    // Lower diagonals
    for(int start=1; start<r; start++) {
        int i=start, j=c-1;
        while(i<r && j>=0) {
            printf("%d ", a[i][j]);
            i++; j--;
        }
        printf("\n");
    }

    return 0;
}

// 80. multiply two matrices.
#include <stdio.h>

int main() {
    int r1,c1,r2,c2;
    scanf("%d %d", &r1, &c1);
    scanf("%d %d", &r2, &c2);

    if(c1 != r2) {
        printf("Multiplication not possible");
        return 0;
    }

    int a[r1][c1], b[r2][c2], m[r1][c2];

    for(int i=0; i<r1; i++)
        for(int j=0; j<c1; j++)
            scanf("%d", &a[i][j]);

    for(int i=0; i<r2; i++)
        for(int j=0; j<c2; j++)
            scanf("%d", &b[i][j]);

    for(int i=0; i<r1; i++)
        for(int j=0; j<c2; j++) {
            m[i][j] = 0;
            for(int k=0; k<c1; k++)
                m[i][j] += a[i][k] * b[k][j];
        }

    for(int i=0; i<r1; i++) {
        for(int j=0; j<c2; j++)
            printf("%d ", m[i][j]);
        printf("\n");
    }
    return 0;
}
